{
  "base_branch": "main",
  "max_attempts": 3,
  "commit_message": "feat(agent): resolve issue #{issue_number}",
  "quality_gates": [
    "git diff --check"
  ],
  "templates": {
    "planner": "prompts/planner.md",
    "coder": "prompts/coder.md",
    "reviewer": "prompts/reviewer.md",
    "pr_body": "templates/pr_body.md"
  },
  "commands": {
    "planner": "$AGENT_PLANNER_CMD",
    "coder": "$AGENT_CODER_CMD",
    "reviewer": "$AGENT_REVIEWER_CMD"
  },
  "target_repo_defaults": {
    "entire": {
      "required": false,
      "explicit_registration": {
        "required": false
      }
    },
    "pr": {
      "labels": [
        "agent/",
        "agent-task"
      ]
    }
  },
  "entire": {
    "enabled": false,
    "required": false,
    "command": "entire",
    "agent": "codex",
    "strategy": "manual-commit",
    "scope": "project",
    "verify_trailer": true,
    "trailer_key": "Entire-Checkpoint",
    "explicit_registration": {
      "enabled": false,
      "required": false,
      "artifact_path": ".entire/evidence/issue-{issue_number}-{run_timestamp}.md",
      "append_commit_trailers": true,
      "max_chars_per_section": 6000,
      "generate_explain": true
    }
  },
  "codex_commit_summary": {
    "enabled": true,
    "required": true,
    "max_chars_per_item": 240,
    "max_attempts": 5,
    "max_points": 3,
    "max_total_chars": 3000
  },
  "ui_evidence": {
    "enabled": true,
    "required": true,
    "delivery_mode": "artifact-only",
    "repo_dir": ".flowsmith/ui-evidence",
    "artifact_dir": "ui-evidence",
    "ui_extensions": [
      ".html",
      ".css",
      ".scss",
      ".sass",
      ".less",
      ".jsx",
      ".tsx",
      ".vue",
      ".svelte",
      ".astro"
    ],
    "ui_path_keywords": [
      "/components/",
      "/pages/",
      "/ui/",
      "/frontend/",
      "/web/",
      "/client/"
    ],
    "image_extensions": [
      ".png",
      ".jpg",
      ".jpeg",
      ".webp",
      ".gif"
    ],
    "evidence_path_keywords": [
      "/ui-evidence/",
      "/screenshots/",
      "/docs/images/ui",
      "/docs/images/screenshot",
      "/docs/images/capture"
    ],
    "evidence_name_keywords": [
      "screenshot",
      "snapshot",
      "capture",
      "screen-",
      "screen_",
      "ui-",
      "ui_"
    ],
    "max_ui_files": 8,
    "max_images": 5
  },
  "ai_logs": {
    "enabled": true,
    "required": true,
    "path": "ai-logs/issue-{issue_number}-{run_timestamp}",
    "index_file": "index.md",
    "publish": {
      "mode": "dedicated-branch",
      "branch": "agent-ai-logs",
      "required": true,
      "commit_message": "chore(agent-logs): store issue #{issue_number} logs ({run_timestamp})"
    }
  },
  "pr": {
    "title": "{pr_title_default}",
    "labels": [
      "agent/",
      "agent-task"
    ],
    "draft": true
  }
}
